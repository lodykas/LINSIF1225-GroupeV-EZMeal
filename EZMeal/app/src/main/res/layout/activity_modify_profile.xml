<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/passwordedit2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/passwordlog2"
        android:layout_toRightOf="@+id/button201"
        android:background="@color/white"
        android:hint="@string/password"
        android:inputType="textPassword"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/passwordlog"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button201"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="300dp"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:text="@string/newPassword"
        android:textColor="@android:color/holo_red_dark" />

    <EditText
        android:id="@+id/passwordedit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/passwordlog"
        android:layout_alignLeft="@id/passwordedit2"
        android:layout_alignStart="@id/passwordedit2"
        android:layout_toEndOf="@+id/button201"
        android:layout_toRightOf="@id/passwordlog"
        android:background="@color/white"
        android:hint="@string/password"
        android:inputType="textPassword"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:textSize="12sp" />

    <TextView
        android:id="@+id/passwordlog2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button201"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="250dp"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:text="@string/confirmNewPassword"
        android:textColor="@android:color/holo_red_dark" />

    <Spinner
        android:id="@+id/spinner1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_alignBaseline="@+id/country"
        android:layout_marginBottom="20dp"
        android:layout_toEndOf="@+id/button201"
        android:layout_toRightOf="@+id/button201"
        android:paddingBottom="0dp">


    </Spinner>

    <TextView
        android:id="@+id/country"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_above="@+id/button201"
        android:layout_alignParentEnd="false"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="false"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="200dp"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:text="@string/newLiving"
        android:textColor="@android:color/holo_red_dark" />

    <Button
        android:id="@+id/button201"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="50dp"
        android:text="OK" />

    <TextView
        android:id="@+id/Messageformodifprofil"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/passwordlog"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/spinner1"
        android:layout_alignStart="@+id/passwordlog"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="84dp"
        android:ems="10"
        android:text="@string/informationsModify"
        android:textAlignment="center"
        android:textColor="@android:color/holo_red_dark"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/usermail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button201"
        android:text="@string/yourUsermail"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="140dp"/>

    <TextView
        android:id="@+id/usermailshow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button201"
        android:text=" : "
        android:layout_marginRight="20dp"
        android:layout_toRightOf="@id/button201"
        android:layout_toEndOf="@id/button201"
        android:layout_marginBottom="140dp" />

    <TextView
        android:id="@+id/origincountry"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button201"
        android:text="@string/yourOrigin"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="100dp"/>

    <TextView
        android:id="@+id/countryoriginshow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button201"
        android:text=" : "
        android:layout_marginRight="20dp"
        android:layout_toRightOf="@id/button201"
        android:layout_toEndOf="@id/button201"
        android:layout_marginBottom="100dp" />

    <TextView
        android:id="@+id/birthday"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button201"
        android:text="@string/yourBirthday"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="60dp"/>

    <TextView
        android:id="@+id/birthdayshow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button201"
        android:text=" : "
        android:layout_marginRight="20dp"
        android:layout_toRightOf="@id/button201"
        android:layout_toEndOf="@id/button201"
        android:layout_marginBottom="60dp" />

    <TextView
        android:id="@+id/allergenes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button201"
        android:text="@string/yourAllergenes"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="15dp"/>

    <TextView
        android:id="@+id/allergenesshow"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/button201"
        android:text=" : "
        android:layout_marginRight="20dp"
        android:layout_toRightOf="@id/button201"
        android:layout_toEndOf="@id/button201"
        android:layout_marginBottom="5dp" />

    <android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.lsinf1225.ezmeal.ezmeal.ModifyProfileActivity"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true">

        <android.support.design.widget.AppBarLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:theme="@style/AppBarOverlay">

            <android.support.v7.widget.Toolbar
                android:id="@+id/toolbar"
                android:layout_width="match_parent"
                android:layout_height="?attr/actionBarSize"
                android:background="@color/colorAccent"
                app:popupTheme="@style/PopupOverlay" />

        </android.support.design.widget.AppBarLayout>

        <include
            android:id="@+id/include"
            layout="@layout/content_menuetage" />

    </android.support.design.widget.CoordinatorLayout>


</RelativeLayout>
